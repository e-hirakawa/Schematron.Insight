package jp.gr.java_conf.schkit.validation;

public enum ResultStatus {
	None(0, "None"),
	SyntaxError(1 << 0, "Syntax Error"),
	Assert(1 << 1, "Assert"),
	Report(1 << 2, "Report");

	private final int id;
	private final String name;

	private ResultStatus(int id, String name) {
		this.id = id;
		this.name = name;
	}
	public int getId()
	{
		return id;
	}
	public String getName()
	{
		return name;
	}
}
