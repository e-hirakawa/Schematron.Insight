package jp.gr.java_conf.schkit;

public enum ExportFormats {
	Log(0, "Log"),
	Tab(1 << 0, "Tab"),
	Xml(1 << 1, "Xml"),
	Json(1 << 2, "Json"),
	Html(1 << 3, "Html");

	private final int id;
	private final String name;
	private ExportFormats(int id, String name)
	{
		this.id = id;
		this.name= name;
	}
	public int getId()
	{
		return id;
	}
	public String getName()
	{
		return name;
	}
}
